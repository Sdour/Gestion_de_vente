<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>User</managed-bean-name>
  <managed-bean-class>DAO.User</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>name</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>password</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>profil</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Article</managed-bean-name>
  <managed-bean-class>DAO.Article</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>descart</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>nomart</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>prixart</property-name>
   <property-class>java.lang.Float</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Command</managed-bean-name>
  <managed-bean-class>DAO.Command</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>datecmd</property-name>
   <property-class>java.util.Date</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>quantitecmd</property-name>
   <property-class>java.lang.Long</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>codeart</property-name>
   <property-class>java.lang.Integer</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>codeclient</property-name>
   <property-class>java.lang.Integer</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>Client</managed-bean-name>
  <managed-bean-class>DAO.Client</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>nomclient</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>tel</property-name>
   <property-class>java.lang.Long</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <display-name>User_login</display-name>
  <from-view-id>/User_login.jsp</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>User_login</display-name>
  <from-view-id>/User_login.jsp</from-view-id>
  <navigation-case>
   <from-outcome>fail</from-outcome>
   <to-view-id>/User_error.jsp</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>AddArticle</display-name>
  <from-view-id>/AddArticle.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addsucced</from-outcome>
   <to-view-id>/Articles.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>AddArticle</display-name>
  <from-view-id>/AddArticle.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addfailed</from-outcome>
   <to-view-id>/AddArticle.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>AddCmd</display-name>
  <from-view-id>/AddCmd.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addcmdfailed</from-outcome>
   <to-view-id>/AddCmd.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>addcmdsucced</from-outcome>
   <to-view-id>/Commands.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>EditClient</display-name>
  <from-view-id>/EditClient.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addclientsucced</from-outcome>
   <to-view-id>/Clients.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>EditClient</display-name>
  <from-view-id>/EditClient.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addclientfailed</from-outcome>
   <to-view-id>/EditClient.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/EditUser.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addusersucced</from-outcome>
   <to-view-id>/User_login.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>adduserfailed</from-outcome>
   <to-view-id>/EditUser.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Commands.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Addcmd</from-outcome>
   <to-view-id>/AddCmd.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>addcmdsucced</from-outcome>
   <to-view-id>/Commands.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/User_error.jsp</from-view-id>
  <navigation-case>
   <from-outcome>fail</from-outcome>
   <to-view-id>/User_error.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
 <!-- 	<factory> 
 		<lifecycle-factory id="nocache">DAO.CacheControlPhaseListener</lifecycle-factory> 
 	</factory> -->
</faces-config>
